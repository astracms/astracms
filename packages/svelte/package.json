{
  "name": "@astracms/svelte",
  "version": "1.0.2",
  "description": "Svelte stores and SvelteKit utilities for AstraCMS",
  "type": "module",
  "private": false,
  "svelte": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --external svelte --external @astracms/core",
    "dev": "tsup src/index.ts --format esm --dts --watch --external svelte --external @astracms/core",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "svelte",
    "sveltekit",
    "stores",
    "astracms",
    "cms",
    "headless-cms",
    "blog"
  ],
  "author": "AstraCMS",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/astracms/astracms",
    "directory": "packages/svelte"
  },
  "peerDependencies": {
    "svelte": "^4.0.0 || ^5.0.0"
  },
  "dependencies": {
    "@astracms/core": "workspace:*"
  },
  "devDependencies": {
    "svelte": "^5.0.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.2"
  }
}
