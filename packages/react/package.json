{
  "name": "@astracms/react",
  "version": "1.0.3",
  "description": "React hooks for AstraCMS data fetching",
  "type": "module",
  "private": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --external react --external @astracms/core",
    "dev": "tsup src/index.ts --format esm --dts --watch --external react --external @astracms/core",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "react",
    "hooks",
    "astracms",
    "cms",
    "headless-cms",
    "blog"
  ],
  "author": "AstraCMS",
  "license": "MIT",
  "homepage": "https://docs.astracms.dev",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@astracms/core": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "react": "^19.0.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.2"
  }
}
