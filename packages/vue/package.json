{
  "name": "@astracms/vue",
  "version": "1.0.3",
  "description": "Vue 3 composables for AstraCMS data fetching",
  "type": "module",
  "private": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --external vue --external @astracms/core",
    "dev": "tsup src/index.ts --format esm --dts --watch --external vue --external @astracms/core",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "vue",
    "vue3",
    "composables",
    "astracms",
    "cms",
    "headless-cms",
    "blog"
  ],
  "author": "AstraCMS",
  "license": "MIT",
  "homepage": "https://docs.astracms.dev",
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "dependencies": {
    "@astracms/core": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.3.0",
    "typescript": "^5.7.2",
    "vue": "^3.5.0"
  }
}
